<% layout('layouts/util-wchat') %>

<div>
    <h2 class="text-xl text-center mb-6 font-bold">GIFs</h2>
    <ul class="flex flex-wrap space-x-3">
    <% gifs.forEach(g => { %>
        <li class="w-32">
            <img src="<%= g.path %>" width="100" />
            <p class="max-w-full"><%= g.title %></p>
        </li>
    <% }) %>
    </ul>
</div>